{
	"name": "friendsoftypo3/form-legacy",
	"type": "typo3-cms-extension",
	"description": "Fork from friendsoftypo3/form-legacy / Test to enable it for 9.x ",
	"homepage": "https://typo3.org",
	"license": ["GPL-2.0+"],

	"require": {
		"typo3/cms-core": "~9"
	},
	"replace": {
		"form_legacy": "*"
	},
	"autoload": {
		"psr-4": {
			"TYPO3\\CMS\\Form\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"TYPO3\\CMS\\Form\\Tests\\": "Tests/"
		}
	}
}
