{
	"name": "friendsoftypo3/form-legacy",
	"type": "typo3-cms-extension",
	"description": "Legacy version 7 of ext:form",
	"homepage": "https://extensions.typo3.org/extension/form_legacy",
	"support": {
		"issues": "https://github.com/FriendsOfTYPO3/form_legacy/issues",
		"source": "https://github.com/FriendsOfTYPO3/form_legacy",
		"docs": "https://docs.typo3.org/p/friendsoftypo3/form-legacy/main/en-us/"
	},
	"license": ["GPL-2.0+"],

	"require": {
		"typo3/cms-core": "~8"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "form_legacy"
		}
	},
	"autoload": {
		"psr-4": {
			"TYPO3\\CMS\\Form\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"TYPO3\\CMS\\Form\\Tests\\": "Tests/"
		}
	}
}
